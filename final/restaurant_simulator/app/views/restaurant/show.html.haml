.row-fluid
  .col-md-2#tablewaitermap
    = render 'waiters/onduty'
  .col-md-10#main
    %h2.text-center TABLES
    %canvas
.row-fluid
  .col-md-5#notification
    %h2 Notification Area
    %p notifications
  .col-md-2#waiters
    %h2 Waiters
    %ul.list-group
    - @waiter.each do |waiter|
      = render 'waiters/offduty'
  .col-md-5#status
    = render 'status'